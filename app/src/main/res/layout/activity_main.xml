<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:sob="http://schemas.android.com/apk/res-auto"
    tools:context=".MainActivity"
    android:background="@color/lightseagreen">


    <com.jmf.mymadeview.customview.InputNumberView
        android:id="@+id/input_NumberView"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        sob:step="5"
        sob:disable="true"
        sob:btnBackground="@color/colorPrimary"
        sob:defaultValue="0"
        android:visibility="gone"
        />

    <com.jmf.mymadeview.customview.customview.loginpage.LoginKeyboard
        android:id="@+id/number_key_pad"
        android:layout_centerInParent="true"
        android:layout_width="200dp"
        android:layout_height="300dp"/>

</RelativeLayout>